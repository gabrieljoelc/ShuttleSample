<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="serviceBus" type="Shuttle.ESB.Core.ServiceBusSection, Shuttle.ESB.Core"/>
  </configSections>

  <appSettings>
    <add key="ServiceBusTransactionScopeIsolationLevel" value="ReadCommitted"/>
    <add key="ServiceBusTransactionScopeTimeoutSeconds" value="15"/>
    <add key="SubscriptionManagerSecured" value="false" />
  </appSettings>

  <connectionStrings>
    <clear/>
    <add
      name="Subscription"
      connectionString="Data Source=.\sqlexpress;Initial Catalog=shuttle;Integrated Security=SSPI;"
      providerName="System.Data.SqlClient"/>
  </connectionStrings>

  <serviceBus>
    <inbox
      workQueueUri="msmq://./pubsub-subscriber2-inbox-work"
      journalQueueUri="msmq://./pubsub-subscriber2-inbox-journal"
      errorQueueUri="msmq://./shuttle-samples-error"
      />
  </serviceBus>
</configuration>
